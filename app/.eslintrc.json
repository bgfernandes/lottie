{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "all",
        "ignoreRestSiblings": false,
        "argsIgnorePattern": "^_"
      }
    ],
    "semi": ["error", "never"],
    "quotes": ["error", "single"]
  }
}
