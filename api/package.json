{
  "name": "lottie-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch './src/**/*.*' --exec 'yarn run-dev'",
    "run-dev": "ts-node ./src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "db:migrate": "knex migrate:latest",
    "db:migrate:rollback": "knex migrate:rollback",
    "db:setup": "yarn db:create && yarn db:migrate",
    "db:create": "ts-node db/createDatabase.ts "
  },
  "dependencies": {
    "apollo-datasource": "^3.1.0",
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.2",
    "graphql-upload": "^12.0.0",
    "json-schema": "^0.3.0",
    "knex": "^0.95.10",
    "mime-types": "^2.1.32",
    "moment": "^2.29.1",
    "nanoid": "^3.1.25",
    "objection": "^2.2.15",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/graphql-upload": "^8.0.7",
    "@types/jest": "^27.0.1",
    "@types/json-schema": "^7.0.9",
    "@types/knex-cleaner": "^1.3.2",
    "@types/mime-types": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-jest": "^27.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.1.0",
    "knex-cleaner": "^1.3.1",
    "nodemon": "^2.0.12",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "4.4.2"
  }
}
